function createPrice(prices) {
    var p = new AdvancedPricing();
    for (var i in prices) {
        var price = prices[i];
        p.addPrice(price.price[0].gross, price.price[0].net, price.quantityStart, price.quantityEnd);
    }
    p.calcDiscounts();
    return p;
}

